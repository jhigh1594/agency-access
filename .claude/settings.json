{
  "permissions": {
    "allow": [
      "Bash",
      "Edit",
      "Skill(dotai:*)"
    ]
  },
  "enableAllProjectMcpServers": true,
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "if [ -f \".claude/scripts/session-start.sh\" ]; then bash .claude/scripts/session-start.sh; fi"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "if [ -f \".claude/scripts/post-compact.sh\" ]; then bash .claude/scripts/post-compact.sh; fi"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "if [ -f \".claude/scripts/user-prompt-submit.sh\" ]; then bash .claude/scripts/user-prompt-submit.sh; fi"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "ctx@dotai": true,
    "dotai@dotai": true,
    "fb@dotai": false,
    "notification@dotai": false,
    "media@dotai": false,
    "codex@dotai": false,
    "plan@dotai": true,
    "agents@dotai": false,
    "debug@dotai": false,
    "test@dotai": false,
    "skills@dotai": true,
    "git@dotai": false,
    "typescript-lsp@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "github@claude-plugins-official": true,
    "vercel@claude-plugins-official": true,
    "supabase@claude-plugins-official": true,
    "sentry@claude-plugins-official": false,
    "linear@claude-plugins-official": true,
    "figma@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "coderabbit@claude-plugins-official": true,
    "backend-development@claude-code-workflows": true,
    "web-scripting@claude-code-workflows": true
  },
  "extraKnownMarketplaces": {
    "dotai": {
      "source": {
        "source": "github",
        "repo": "udecode/dotai"
      }
    },
    "anthropics-claude-code": {
      "source": {
        "source": "github",
        "repo": "anthropics/claude-code"
      }
    }
  }
}
