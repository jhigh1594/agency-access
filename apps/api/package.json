{
  "name": "@agency-platform/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json src/index.ts",
    "build": "tsc && tsc-alias --resolve-full-paths",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "db:security:check": "node ./scripts/check-neon-security.mjs",
    "bench:security": "node ./scripts/benchmark-security-endpoints.mjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@agency-platform/shared": "^0.1.0",
    "@aws-sdk/client-secrets-manager": "^3.985.0",
    "@clerk/backend": "^1.19.1",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@infisical/sdk": "^4.0.6",
    "@prisma/client": "^6.2.1",
    "axios": "^1.13.5",
    "bullmq": "^5.38.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.7.4",
    "fastify-raw-body": "^5.0.0",
    "ioredis": "^5.4.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.1.3",
    "resend": "^6.6.0",
    "tsc-alias": "^1.8.16",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.19.3",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.17.0",
    "glob": "^13.0.3",
    "prisma": "^6.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "axios": "^1.13.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
